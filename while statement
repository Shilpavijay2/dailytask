while statement


using System;
public class HelloWorld {
  public static void Main() 
  {
      int i = 2;
      Console.WriteLine("i  j");
      while(i<=3)
      {
          int j = 1;
          while(j<=5)
          {
            Console.WriteLine(i+ " " +j);
            j++;
          }
        i++;
      }
      Console.ReadLine();
  }
}
