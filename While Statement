While Statement


using System;
public class HelloWorld
{
 public static void Main(string[] args) 
 {
     int i = 1;
     Console.WriteLine("i  j");
     while(i<=2)
      {
         int j = 1;
         while(j<=4)
         {
            Console.WriteLine(i + " "+ j);
            j++;
         }
         i++;
      }
    
      Console.ReadLine();
  }
}
